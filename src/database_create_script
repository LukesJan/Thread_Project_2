create database csv_import;
CREATE USER 'tDB'@'localhost' IDENTIFIED BY '1234';
GRANT ALL PRIVILEGES ON csv_import.* TO 'tDB'@'localhost';
FLUSH PRIVILEGES;

use csv_import;

CREATE TABLE products (
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(100),
    category VARCHAR(50),
    price DECIMAL(10,2),
    quantity INT
);

select count(id) as pocet
from products;

drop table products;